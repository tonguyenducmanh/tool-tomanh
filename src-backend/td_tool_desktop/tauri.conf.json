{
  "$schema": "../../node_modules/@tauri-apps/cli/config.schema.json",
  "productName": "tool-tomanh",
  "version": "0.1.0",
  "identifier": "com.tomanh.tool",
  "build": {
    "frontendDist": "../dist",
    "devUrl": "http://localhost:5173",
    "beforeDevCommand": "npm run web:dev",
    "beforeBuildCommand": "npm run web:build"
  },
  "app": {
    "windows": [
      {
        "title": "tool-tomanh",
        "width": 1920,
        "height": 1080,
        "maximizable": true,
        "resizable": true,
        "fullscreen": false
      }
    ],
    "security": {
      "csp": null
    }
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ]
  }
}
